<H1>CONTACTO</H1>
<form action="" class="form-contact">
   <div class="box-nombre">
      <label for="name" id="nomrecontact">Nombre</label>
      <input type="text" name="name" id="input_nombre">
   </div>
   <div class="box-email">
      <label for="email">Correo</label>
      <input type="email" name="email" id="email">
   </div>
   <div class="box-number">
      <label for="phone">Telefono</label>
      <input type="numer" name="phone" id="phone">
   </div>
   <div class="box-asunto">
      <label for="asunto">Asunto</label>
      <input type="text" name="asunto" id="asunto">
   </div>
   <div class="box-textarea">
      <label for="mensaje" id="mensaje">Mensaje</label>
      <textarea name="mensaje" id="mensaje" cols="30" rows="10"></textarea>
   </div>
</form>
